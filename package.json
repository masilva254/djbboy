    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@types/multer": "^1.4.7",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.4",
    "eslint": "8.57.0",
    "eslint-config-next": "15.0.0",
    "husky": "^9.0.10",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<yourusername>/mixhub.git"
